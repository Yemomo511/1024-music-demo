{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "experimentalDecorators": true /* Enables experimental support for ES7 decorators. */,
    "baseUrl": ".", // 配置基础地址
    "paths": {
      "@": ["src/*"],
      "@assets": ["src/assets"],
      "@components": ["src/components"],
      "@modules": ["src/modules"],
      "@stores": ["src/stores"],
      "@utils": ["src/utils"]
    }
  }
}
