{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "baseUrl": "src",
    "paths": {
      "~component/*":["components/*"],
      "~assets/*":["assets/*"],
      "~screen/*":["screen/*"],
      "~image/*":["image/*"]
    },
  },
  "exclude": [
    "metro.config.js",
  ]
}
